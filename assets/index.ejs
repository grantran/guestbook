    <div class='forms'>
      <form id='guest-entry'>
        <div class="form-group">
          <label for="first-name">First Name</label>
          <input type="text" class="form-control" id="first-name" placeholder="First Name" style="width:500px">
        </div>
        <div class="form-group">
          <label for="last-names">Last Name</label>
          <input type="text" class="form-control" id="last-name" placeholder="Last Name" style="width:500px">
        </div>
        <div>
        <select id='membership'>
          <option value="member">Member</option>
          <option value="non-member">Non-Member</option>
        </select>
        </div>
        <br/>
        <button type="submit" class="btn btn-primary">Submit</button>
      </form> 
    </div>
    <br/>
    <div class="container">
      <div class="row">
        <div class="col-sm-6 container" id='member-name-list'>
          <h3>Members</h3>

        </div>
        <div class="col-sm-6 container" id='non-member-name-list'>
          <h3>Non-Members</h3>
          <div>

          </div>

        </div>
      </div>
